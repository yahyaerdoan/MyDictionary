@model List<Dictionary.EntityLayer.Concrete.Writer>

<div class="card card-primary card-outline">
    @foreach (var item in Model)
    {
        <div class="card-body box-profile">
            <div class="text-center">
                <img class="profile-user-img img-fluid img-circle"
                     src="~/Template/AdminLTE-3.2.0/dist/img/user4-128x128.jpg"
                     alt="User profile picture">
            </div>
            <h3 class="profile-username text-center"></h3>
            <p class="text-muted text-center">@item.Title</p>
            <p class="text-bold text-center">@item.FirstName @item.LastName</p>
            <ul class="list-group list-group-unbordered mb-3">
                <li class="list-group-item">
                    <b>Followers</b> <a class="float-right">1,322</a>
                </li>
                <li class="list-group-item">
                    <b>Following</b> <a class="float-right">543</a>
                </li>
                <li class="list-group-item">
                    <b>Friends</b> <a class="float-right">13,287</a>
                </li>
            </ul>
            <a href="#" class="btn btn-primary btn-block"><b>Follow</b></a>
        </div>
    }
</div>